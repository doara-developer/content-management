    "main": "index.js",
    "scripts": {
        "build": "run-p build:*",
        "build:client": "webpack --config ./webpack.config.client.js",
        "build:server": "webpack --config ./webpack.config.server.js",
        "watch:client": "webpack --config ./webpack.config.client.js --watch",
        "start": "node dist/app.js",
        "dev": "ts-node src/server/app.ts",
        "json-server": "json-server --watch mock/apiMock.json --port 5000 --routes mock/routes.json",
        "lint": "run-p lint:*",
        "lint:client": "eslint --ext .ts,.vue src/client/",
        "lint:server": "eslint --fix src/server/**/*.ts",
        "test": "run-p test:*",
        "test:client": "jest --config jest.config.client.js --updateSnapshot",
        "test:server": "jest --config jest.config.server.js --updateSnapshot",
        "coverage": "run-p coverage:*",
        "coverage:client": "jest --config jest.config.client.js --collectCoverage --updateSnapshot --coverageDirectory coverage/client",
        "coverage:server": "jest --config jest.config.server.js --collectCoverage --updateSnapshot --coverageDirectory coverage/server"
    },
